void setup()
{
Serial.begin(9600);
}
void loop()
{
  if(Serial.available()>0){
    int a =Serial.parseInt();
    int b =Serial.parseInt();
    if(a>b){
      Serial.println(a);
      Serial.println("a is the largest number between a and b");
    }
     else{
       Serial.println(b);
      Serial.println("b is the largest number between a and b");
       
     }
 }
}
